    <!-- COMMON_UMAKE_MSPSDK -->
    <module id="common_umake_mspsdk" name="common/umake/mspsdk" group="core"> 

        <attribute id="no_build"/> 
        <cvs root="helix"/>

        <sdk name = "ms_platformsdk" path = "c:\Program Files\Microsoft Platform SDK for Windows XP SP2">
            ERROR: This module requires the latest Microsoft Platform SDK (June 2004). Please
            visit http://www.microsoft.com/downloads/details.aspx?familyid=9be921b3-585e-47d9-bcc1-980879b69b34
            to install the package.

            You may need to add a line to your.buildrc as follows that specifies the install path.
         
            SetSDKPath("ms_platformsdk", "c:\\program files\\Microsoft Platform SDK for Windows XP SP2")

            Normally you will only need to do this if you install to a non-default location.
        </sdk>

        <sdk name = "ms_platformsdk" path = "c:\Program Files\Microsoft SDK">
            ERROR: This module requires the Microsoft Platform SDK (Feb 2003 or later). Please
            visit http://www.microsoft.com/msdownload/platformsdk/sdkupdate/ and install the
            core sdk package.

            You may need to add a line to your.buildrc as follows that specifies the install path.
         
            SetSDKPath("ms_platformsdk", "c:\\program files\\microsoft sdk")

            Normally you will only need to do this if you install to a non-default location.
        </sdk>
      
        <!-- The platform sdk is only required when using msvc 6.0 (not .NET) -->
        <includeplatforms>
            win32-i386-vc6
            win32-i386-xicl6
            win32-i386-vc7
            win32-i386-xicl7
            win32-i386-vc8
        </includeplatforms>

        <!-- Unfortunately we can't just list helix-client-core-net(.pfi) here -->
        <includeprofiles>
            helix-client-all-defines
            helix-client-all-defines-free
            helix-client-all-defines-nodrm
            helix-server-all-defines
            helix-dtdr-all-defines
            helix-client-all-defines-nodist
            helix-client-local-net-ra-mp3
            <!-- helix-client-wincce-420 -->
            helix-client-strummer
            helix-client-helixsim
            helix-client-playerx
        </includeprofiles>

        <defines>
            _STLP_NEW_PLATFORM_SDK
            HELIX_CONFIG_MS_PLATFORM_SDK
        </defines>
        <umake_includefiles>
            common/umake/mspsdk/umakefil
        </umake_includefiles>
        
    </module>


