"""IRIX 6.4 using gcc 2.8.1."""

## list of PCF files to run
platform.pcf_prefix_list = ['unix', 'irix6']

## defines
project.AddDefines('_IRIX', 'OLDBSD', '__sgi__', 'IRIX6', '_BIG_ENDIAN', '_STRICT_ALIGNMENT')

## decide to use either GCC of the native compiler
## based on the 'native' build option
if project.build_choices.count('native'):
    exec_config_file('irix-6.2-mips-native.cf')
else:
    exec_config_file('irix-6.2-mips-gcc.cf')

platform.cc.args['default'] = platform.cc.args['default'] + " -mips3 -fpermissive "
platform.cxx.args['default'] = platform.cxx.args['default'] + " -mips3 -fpermissive "
platform.cxx.args['debug'] = " -g -DDEBUG -D_DEBUG "
platform.cc.args['debug'] = " -g -DDEBUG -D_DEBUG "
DisableRTTIIfNecessary()
